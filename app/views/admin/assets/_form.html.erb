<%= simple_form_for([:admin, @asset]) do |f| %>
  <%= f.error_notification %>

  <div class="box-body">
    <div class="row">
      <div class='col-md-6'>
        <%= f.input :description %>
      </div>
      <div class='col-md-6'>
        <%= f.input :file %>
      </div>
    </div>
  </div>

  <div class="box-footer">
    <%= f.button :submit %>
  </div>
<% end %>
