.themecolorCustom .tz-header {
    <% if @preference.header_image_allowed.present? %>
        background-image: url('<%= @preference.header_image.url %>') !important;
    <% else %>
        background-image: @preference.color_1 !important;
    <% end %>
    background-size: cover !important;
    background-position: 50% 50% !important;
}

.themecolorCustom .tz-footer {
    <% if @preference.footer_image_allowed.present? %>
        background-image: url('<%= @preference.footer_image.url %>') !important;
    <% else %>
        background-image: @preference.color_1 !important;
    <% end %>
    background-size: cover !important;
    background-position: 50% 50% !important;
}

.themecolorCustom .tz-header.blog-header {
    <% if @preference.blog_header_image_allowed.present? %>
        background-image: url('<%= @preference.blog_header_image.url %>') !important;
    <% else %>
        background-image: @preference.color_1 !important;
    <% end %>
    background-size: cover !important;
    background-position: 50% 50% !important;
}

.themecolorCustom .tz-footer.blog-footer {
    <% if @preference.blog_footer_image_allowed.present? %>
        background-image: url('<%= @preference.blog_footer_image.url %>') !important;
    <% else %>
        background-image: @preference.color_1 !important;
    <% end %>
    background-size: cover !important;
    background-position: 50% 50% !important;
}

.themecolorCustom .tz-header1 .tzlogo a {
    background: none !important;
}

